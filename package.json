{
	"name": "md-to-pdf",
	"version": "1.2.0",
	"description": "CLI tool for converting Markdown files to PDF",
	"keywords": [
		"markdown-to-pdf",
		"pdf-generation",
		"documentation",
		"assignments",
		"homework",
		"markdown",
		"pdf"
	],
	"engines": {
		"node": ">= 8.3.0"
	},
	"main": "index.js",
	"scripts": {
		"test": "xo && ava"
	},
	"bin": {
		"markdown-to-pdf": "./index.js",
		"md2pdf": "./index.js"
	},
	"author": "Simon Haenisch (https://simonhaenisch.com)",
	"license": "Unlicense",
	"repository": "https://github.com/simonhaenisch/md-to-pdf",
	"dependencies": {
		"arg": "2.0.0",
		"chalk": "2.4.0",
		"gray-matter": "4.0.1",
		"highlight.js": "9.12.0",
		"html-pdf": "2.2.0",
		"iconv-lite": "0.4.21",
		"marked": "0.3.19"
	},
	"devDependencies": {
		"ava": "0.25.0",
		"xo": "0.21.1"
	},
	"prettier": {
		"printWidth": 120,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all",
		"useTabs": true,
		"bracketSpacing": true
	},
	"xo": {
		"prettier": true,
		"rules": {
			"camelcase": ["error", { "properties": "never" }],
			"capitalized-comments": "off",
			"valid-jsdoc": ["warn", { "requireReturnType": false }]
		}
	}
}
